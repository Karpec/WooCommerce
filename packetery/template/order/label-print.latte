<h1>{__( 'Packeta', 'packetery' )} - {__( 'labelPrinting', 'packetery' )}</h1>

{if !$errors}
	<form n:name=$form>
		<table class="form-table" role="presentation">
			<tr>
				<th scope="row">
					<label n:name=offset />
				</th>
				<td>
					<select n:name=offset></select>
				</td>
			</tr>
		</table>
		<p class="submit">
			<input type="submit" name="submit" id="submit" value="{__( 'printLabels', 'packetery' )}"
				   class="button button-primary">
		</p>
	</form>
{/if}
